(state 
  (init
	(initials (shift subsq))
	(otherchars (pushback 1) (shift subsq))
  )
  (subsq
	(allchars 
		(pushback 1)
    (shift letters))
  (backspace)
  )
  (letters
	  (medials (shift subsq))
  )
